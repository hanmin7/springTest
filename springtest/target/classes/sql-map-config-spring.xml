<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<!-- 오라클 컬럼이 board_no를 VO boardNo 처럼 카멜케이스로 변환 --> 
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.yedam.app.member.MemberVo" alias="member"/>
		<typeAlias type="com.yedam.app.board.BoardVo" alias="board"/>
	</typeAliases>

</configuration>
